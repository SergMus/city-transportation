<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>EndTimeRule</fullName>
    <active>true</active>
    <errorConditionFormula>IF(  EndTime__c &lt;  StartTime__c  , true, false)</errorConditionFormula>
    <errorDisplayField>EndTime__c</errorDisplayField>
    <errorMessage>Bus Route End Time should always be later than Start Time</errorMessage>
</ValidationRule>
