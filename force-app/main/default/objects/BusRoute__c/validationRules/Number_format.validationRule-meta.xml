<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Number_format</fullName>
    <active>true</active>
    <errorConditionFormula>REGEX(RouteNumber__c, &quot;[0-9]|[0-9][a-zA-Z][0-9]|[a-zA-Z]$&quot;)</errorConditionFormula>
    <errorDisplayField>RouteNumber__c</errorDisplayField>
    <errorMessage>Incorrect value, only enabled in format &apos;133&apos;, &apos;1A&apos;, &apos;21B&apos; etc.</errorMessage>
</ValidationRule>
